pr = float()
n = int(input())
for x in range(n):
    p, q = map(int,input().split())
    c = int(input())
    if p<1:
        p=1
    elif p>5:
        p=5
    if q<1:
        q = 1
    elif q>500:
        q=500
    
    if c==1001:
        pr = q*1.5
    elif c==1002:
        pr = q*2.5
    elif c==1003:
        pr = q*3.5
    elif c==1004:
        pr=q*4.5
    elif c==1005:
        pr = q*5.5
    pr+=pr
    print(f'{pr:.2f}')
