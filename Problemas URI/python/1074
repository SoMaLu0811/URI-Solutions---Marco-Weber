n = int(input())
n2 = int()
for x in range(n):
    n2 = int(input())
    if n2>0 and n2%2 == 0:
        print("EVEN POSITIVE")
    elif n2>0 and n2%2 != 0:
        print("ODD POSITIVE")
    elif n2<0 and n2%2 == 0:
        print("EVEN NEGATIVE")
    elif n2<0 and n2%2 != 0:
        print("ODD NEGATIVE")
    elif n2 == 0:
        print("NULL")    