le = set()
coun = set()
while True:
    try:
        n,i = map(int, input().split())
        for x in range(n):
            i,j = map(int, input().split())
            if j==0:
                coun.add(j)
            elif j==1:
                le.add(j)
             
        print(f'{len(coun)+1}')
        
    except EOFError:break 