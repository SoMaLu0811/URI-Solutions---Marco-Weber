lista = [2,3,5]
def primo(n):
    if n < 2:
        return False
    for i in range(2, int(n/2)):
        if n%i==0:
            return False
        return True
m = int(input("Digite até qual número deseja verificar os números primos:"))
n = 1
for i in range(m-1):
    n+=1
    primo(n)
    if primo(n):
        lista.append(n)

print(lista)
    
    
   
        
    
    